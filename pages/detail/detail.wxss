video{
    width:100%
}
view.info{
    padding: 10px 20px 0px 15px;
}
image.icon{
    width:28px;
    height:29px;
    border-radius: 100%;
    display: inline-block;
    vertical-align: middle
}
text.name{
    font-size: 14px;
    color: #010101;
    margin-left: 10px;
}
view.title{
    margin-top:6px;
    margin-bottom: 6px;
    font-size: 17px;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    line-height: 24px;
    color: #000000;
}
.image_eye{
  width:16px;
  height:11px;
  margin-right:5px;
}
.eye_num{
    font-size:14px;
}
/* 相关视频 */
.relateContent{
    margin-top:10px;
    margin-bottom:10px;
}
.videoList {
    padding:10px 15px;
    clear:both;
}
.videoLeft{
    float:left;
    width:33.333334%;
}
.videoRight{
    float: left;
    width: 66.666668%;
    position: relative;
    height:67px;
}
.videoImg{
    width:100%;
    height:67px;
}
.videoTitle{
    font-size:14px;
    line-height:17px;
    color:#292929;
    position:absolute;
    top:0px;
    padding-left:10px;
}
.videoInfo{
    position:absolute;
    bottom:0px;
    padding-left:10px;
}
.videoRight .eye_num{
    font-size:12px;
}
